<script type="text/javascript">
	function nl2br (str, is_xhtml) {   
		var breakTag = (is_xhtml || typeof is_xhtml === 'undefined') ? '<br />' : '<br>';    
		return (str + '').replace(/([^>\r\n]?)(\r\n|\n\r|\r|\n)/g, '$1'+ breakTag +'$2');
	}
	
	$(document).on("click",".refund-tg",function() {
		var tid = $(this).attr("data-id");
		$("#refund_order").val(tid);
		$(".confirm-refundbtn").attr("data-id",tid);
		$("#refundModal").modal("show");
	})
	
	$(document).on("click",".rule-chkbox",function() {
		var checked_val = $(".rule-chkbox:checked").val();
		if(checked_val == '2') {
			$("#partial_amount").addClass("required");
			$(".partial-blk").removeClass("d-none");
		} else {
			$("#partial_amount").removeClass("required");
			$(".partial-blk").addClass("d-none");
		}
	})
	
	$(document).on("click",".confirm-refundbtn",function() {
		var tid = $(this).attr("data-id");
		if($("#refund_form").valid()) {
			$.ajax({
				url: baseUrl + "/refund-amount",
				type: "POST",
				data: $("#refund_form").serialize(),
				success: function (data) {
					if(data == 'success') {
						showAppAlert('Success!!','You have successfully placed the refund request.','success');
						setTimeout(function(){ location.reload(); }, 1000);
					} else if(data == 'refund_issued') {
						showAppAlert('Error!!','Refund has already been issued for this product.','error');
					} else if(data == 'invalid_amount') {
						showAppAlert('Error!!','Partial amount should be less than the paid amount excluding processing fee.','error');
					} else {
						showAppAlert('Error!!','No such order has been placed.','error');
					}
				}
			})
		}	
	})
	
	$(document).on('input','#partial_amount', function() {
		this.value = this.value.match(/^\d+\.?\d{0,2}/);	
	})
	
	$(document).on("click",".track-closebtn",function() {
		var tid = $(this).attr("data-id");
		var chk = $(".orderstat-chkbox[data-id="+tid+"]").val();
		var optVal = chk - 1;
		$(".orderstat-chkbox[data-id="+tid+"]").val(optVal)
		$("#trackingModal").modal("hide");	
	})
	
	$(document).on("click",".confirm-cancelbtn",function() {
		var tid = $(this).attr("data-id");
		var chk = $(".orderstat-chkbox[data-id="+tid+"]").val();
		var optVal = chk - 1;
		$(".orderstat-chkbox[data-id="+tid+"]").val(optVal)
		$("#trackingModal").modal("hide");	
	})
	
	$(document).on("change",".orderchk-box",function() {
		var orderchk = $(this).val();
		$.ajax({
			url: baseUrl + "/seller-chkorder",
			type: "POST",
			data: { orderchk: orderchk },
			success: function (data) {
				$(".order-mainblk").html(data);		
			}
		})
