	$(document).on("click",".confirm-trackbtn",function() {
		var tid = $(this).attr("data-id");
		var chk = $(".orderstat-chkbox[data-id="+tid+"]").val();
		var tracktxt = $(".tracking_txt").val();
		$.ajax({
				url: baseUrl + "/order-update",
				type: "POST",
				data: { orderchk: chk,order:tid,tracktxt:tracktxt },
				success: function (data) {
					if(data == 'error') {
						showAppAlert('Error!!','No such order has been placed.','error');
					} else if(data == 'invalid_input') {
						showAppAlert('Error!!','No such order has been placed.','error');
					} else {
						showAppAlert('Success!!','You have successfully updated the order status.','success');
						setTimeout(function(){ location.reload(); }, 1000);
					}
				}
			})
	})
	
